timateInfo.livingroom = info.livingroom;\n            state.estimateInfo.kitchen = info.kitchen;\n            state.estimateInfo.bedroom = info.bedroom;\n            state.estimateInfo.hall = info.hall;\n            state.estimateInfo.basement = info.basement;\n            state.estimateInfo.garage = info.garage;\n            state.estimateInfo.room = info.room;\n            state.estimateInfo.wardrobe = info.wardrobe;\n            state.estimate.push(state.estimateInfo);\n        }\n    },\n    actions: {},\n    modules: {},\n    plugins: [vuexLocal.plugin]\n})\n"]}]}